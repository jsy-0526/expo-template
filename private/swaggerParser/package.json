{
  "name": "swagger-parser",
  "version": "1.0.0",
  "description": "Parse OpenAPI 3.x JSON to extract APIs and type definitions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@9.15.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "example:basic": "tsx examples/basic-usage.ts",
    "example:ref": "tsx examples/ref-resolution.ts",
    "example:codegen": "tsx examples/code-generation.ts"
  },
  "keywords": [
    "swagger",
    "openapi",
    "parser",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.4",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16"
  }
}
